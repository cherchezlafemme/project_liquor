
def run_loop (board_array, cell_number, guess_number)
  loop do
    all_empty_cells = empty_cells_addresses_whole_board(board_array)
    p all_empty_cells
      break if all_empty_cells.length == 0
      cells_and_its_candidates = find_candidates_for_each_empty_cell(board_array, all_empty_cells)


      # if find_candidates_for_each_empty_cell(board_array, all_empty_cells) == false
      #   puts "MOVING ON TO THE SECOND GUESS"

      #   # guess_number = 1
      #   # run_loop(board_array, cell_number, guess_number)

      #   # break  if can_solve_more?(cells_and_its_candidates) == false

      # end

    p can_solve_more?(cells_and_its_candidates)
      if can_solve_more?(cells_and_its_candidates) == false
        puts "LINE 35"
        cells_and_its_candidates.values[cell_number].each do |possibilite_value|


          loop do guess_number = possibilite_value
            p guess_number
            p cells_and_its_candidates
            p all_empty_cells
            puts "LINE 37"
          impliment_advanced_logic(board_array, cells_and_its_candidates, cell_number, guess_number, all_empty_cells)

          break if find_candidates_for_each_empty_cell(board_array, all_empty_cells) == false
          end
        end
      return board_array
      end

    match_finder_updater_board(board_array, cells_and_its_candidates)
  #   return board_array if board_has_empty_cell?(board_array) == false
  # puts "LINE 37 #{board_array}"
    board_array
  end
end
